<% content_for :javascripts do %>
  <script type="text/javascript">
    $(function() {
      window.testers = new Testers(<%= raw TesterPresenter.new(User.testers.unretired).to_json %>);
      var tickets = new Houston.TestingReport.Tickets(<%= raw tickets.to_json %>);
      new Houston.TestingReport.TestingReportView({
        el: '#testing_report',
        projectsCanCloseTicketsFor: <%= raw @projects.select { |project| can?(:close, project.tickets.build) }.map(&:id).to_json %>,
        tickets: tickets
      });
    });
  </script>
<% end %>
